# Dashboard configuration
PORT=3000
BIND_ADDRESS=192.168.50.22

# Session secret (generate with: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))")
SESSION_SECRET=changeme

# Dashboard password (bcrypt hash)
# Generate with: node -e "require('bcrypt').hash('yourpassword', 12).then(h => console.log(h))"
PASSWORD_HASH=changeme

# Session TTL in hours
SESSION_TTL_HOURS=8

# Kubeconfig path (leave empty to use default ~/.kube/config or in-cluster config)
KUBECONFIG_PATH=
